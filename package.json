{
	"name": "coucool-cms",
	"private": true,
	"version": "1.0.0",
	"main": "package.json",
	"license": "UNLICENSED",
	"scripts": {
		"dev": "sanity dev",
		"start": "sanity start",
		"build": "sanity build",
		"deploy": "sanity deploy",
		"deploy-graphql": "sanity graphql deploy",
		"prettier:check": "prettier --check .",
		"prettier:write": "prettier --write .",
		"check-lint": "eslint --ext .js,.jsx,.ts,.tsx --max-warnings=0",
		"check-types": "tsc --noEmit",
		"check-unused-exports": "ts-prune -e",
		"lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx && ts-prune -e",
		"prepare": "husky install",
		"clone-to-production": "sanity dataset export staging staging.tar.gz && sanity dataset import staging.tar.gz production --replace && rm -rf staging.tar.gz",
		"clone-to-staging": "sanity dataset export production production.tar.gz && sanity dataset import production.tar.gz staging --replace && rm -rf production.tar.gz"
	},
	"dependencies": {
		"@sanity/asset-utils": "^1.3.0",
		"@sanity/cli": "^3.6.0",
		"@sanity/color-input": "^4.0.3",
		"@sanity/icons": "^3.5.7",
		"@sanity/image-url": "^1.0.2",
		"@sanity/orderable-document-list": "^1.0.2",
		"@sanity/ui": "^2.0.1",
		"@sanity/vision": "^3.6.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rxjs": "^7.8.1",
		"sanity": "^3.26.1",
		"sanity-pills": "^2.0.0",
		"styled-components": "^6.1.8",
		"ts-prune": "^0.10.3"
	},
	"devDependencies": {
		"@types/node": "^20.10.5",
		"@types/react": "^18.2.48",
		"@types/react-dom": "^18.2.18",
		"@types/styled-components": "^5.1.26",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"eslint": "^8.6.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-no-inline-styles": "^1.0.5",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"husky": "^9.0.7",
		"prettier": "^3.2.4",
		"typescript": "^5.3.3"
	}
}
